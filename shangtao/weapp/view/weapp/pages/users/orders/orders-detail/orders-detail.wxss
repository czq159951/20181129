page{
  width: 100%;
  height: 100%;
  background: #f6f6f8;
  font-size: 28rpx;
}
.container{
  width: 100%;
  height: auto;
}
.detail-head{
  border-bottom: 2rpx solid #eaeaea;
  padding: 50rpx 10rpx 20rpx 10rpx;
  background: #fff;
}
.detail-head2{
  margin-top: 10rpx;
  border-bottom: 2rpx solid #eaeaea;
  padding: 20rpx 10rpx;
  background: #fff;
}
.wst-row-flex{
  display: -webkit-box;
  width: 100%;
}
.wst-row-flex .wst-col {
  float: none;
  -webkit-box-flex: 1;
  width: 0;
  box-sizing: border-box;
}
.wst-or-process{margin-bottom:20rpx;}
.wst-or-process .process{position: relative;}
.wst-or-process p{text-align:center;display:block;}
.wst-or-process .line{margin-bottom:20rpx;padding:20rpx 0;}
.wst-or-process .line span{float:left;width:50%;height:2px;background:#eaeaea;}
.wst-or-process .line span.active{background:#e00102;}
.wst-or-process .icon{position: absolute;left:0;top:0;display: block;width:100%;height:40rpx;}
.wst-or-process .icon icon{height:40rpx;line-height:40rpx;background: #fff;}
.wst-or-de .term{
  padding: 5rpx 0;
  background:#fff;
}
.wst-or-de .term .left{
  float:left;
  width:20%;
  line-height: 40rpx;
}
.wst-or-de .term .right{
  float:left;
  width:80%;
  text-align:left;
  line-height: 40rpx;
}
.wst-red{
  color: #e00102;
}
.wst-ash{
  color: #9a9a9a;
}
.wst-or-de .local{
  position: relative;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
  padding-left:30rpx;
  color: #9a9a9a;
}
.wst-or-de .local .icon{
  position: absolute;
  left: 1rpx;
  top: 0;
  display: block;
  width: 18rpx;
  height: 50rpx;
  background-size: 100%;
}
.wst-or-de .shops{
  padding: 10rpx 0;
}
.wst-or-de .shops i{
  float:left;
  width:44rpx;
  height:40rpx;
  position:relative;
  background:url('data:image/png;base64,') 0 4rpx no-repeat;
  background-size:87%;
}
.wst-or-de .goods{
  padding: 10rpx;
  border-top: 1rpx solid #ededef;
}
.wst-or-de .goods .img{
  float:left;
  width: 130rpx;
  height: 130rpx;
}
.wst-or-de .goods .img image{
  width: 130rpx;
  height: 130rpx;
}
.wst-or-de .goods .names{
  float:left;
  width: 400rpx;
  padding: 10rpx 10rpx 0 10rpx;
}
.wst-or-de .goods .name{
  width: 400rpx;
  height: 80rpx;
  line-height: 40rpx;
  font-size: 28rpx;
  display: -webkit-box;
  overflow: hidden;
  text-overflow: ellipsis;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
}
.wst-or-de .goods .spec{
  width: 400rpx;
  height: 40rpx;
  line-height: 40rpx;
  font-size: 26rpx;
  color: #ccc;
  text-overflow: ellipsis;
  white-space: nowrap;
  overflow: hidden;
}
.wst-or-de .goods .price{
  float:left;
  width: 140rpx;
  font-size: 28rpx;
  padding: 20rpx 0;
  text-align: right;
}
.wst-or-de .number{
  margin-top: 20rpx;
  padding: 20rpx 0;
  border-top: 1rpx solid #f2f1f1;
}
.wst-or-de .number .price{
  font-size: 34rpx;
  font-weight: bold;
}
.address{
  background: url('data:image/png;base64,') no-repeat 0 6rpx;
  background-size:100%;
}